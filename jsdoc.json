{
  "disqus": "",
  "googleAnalytics": "",
  "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
  },
  "meta": {
      "title": "",
      "description": "",
      "keyword": ""
  },
  "linenums": true,
  "source": {
    "include": ["./src/js/bd", "./src/js/fsnbase"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
      "encoding": "utf8",
      "recurse": true,
      "private": false,
      "lenient": true,
      "destination": "./docs",
      "template": "./node_modules/egjs-jsdoc-template"
  },
  "plugins": ["plugins/group"],
  "templates": {
		"cleverLinks": true,
		"monospaceLinks": true,
		"default": {
			"outputSourceFiles" : true
		},
		"applicationName": "Fsn Phaser",
		"disqus": "",
		"openGraph": {
			"title": "",
			"type": "website",
			"image": "",
			"site_name": "",
			"url": ""
		},
		"meta": {
			"title": "",
			"description": "",
			"keyword": ""
		},
    "defaultLanguage" : "ko",
		"linenums": true
	}
}
